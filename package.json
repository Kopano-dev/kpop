{
  "name": "kpop",
  "version": "0.0.5",
  "jsnext:main": "es/index.js",
  "module": "es/index.js",
  "private": true,
  "sideEffects": false,
  "files": [
    "es",
    "static",
    ".version"
  ],
  "scripts": {
    "build": "nwb build-react-component --copy-files --no-demo",
    "clean": "nwb clean-module && nwb clean-demo",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "test": "jest --watch"
  },
  "dependencies": {
    "prop-types": "^15.6.1"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.3",
    "nwb": "0.21.x",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-styleguidist": "^7.0.5",
    "react-test-renderer": "^16.2.0"
  },
  "engines": {
    "node": ">=6.11.0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "dist/.*",
      "demo/.*",
      "es/.*",
      "lib/.*"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/jest.transform.js"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  }
}
